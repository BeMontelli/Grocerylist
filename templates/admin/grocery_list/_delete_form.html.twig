<form method="post" action="{{ path('admin.list.delete', {'id': grocery_list.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ grocery_list.id) }}">
    <button class="btn-primary btn">{{ 'app.admin.action.delete'|trans }}</button>
</form>
